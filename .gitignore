# .gitignore

# Ignore the node_modules directory and build output in the frontend
/frontend/node_modules/
/frontend/build/
/frontend/dist/
/frontend/.env

# Ignore the Python cache and database in the backend
/backend/__pycache__/
/backend/*.py[cod]
/backend/*.sqlite3
/backend/db.sqlite3
/backend/media/
/backend/staticfiles/
/backend/.env
/backend/venv


# Ignore system files
.DS_Store
Thumbs.db

# Ignore virtual environment directory
venv/
.env/

# Ignore IDE configuration files
.idea/
.vscode/
*.iml

# Ignore log files and temp files
*.log
*.tmp
*.bak
*.swp
*.swo

# Ignore coverage reports
coverage/
*.cover
.hypothesis/
.pytest_cache/

# Ignore pytest cache
__pycache__/
